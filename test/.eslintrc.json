{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "extends": ["eslint:recommended", "plugin:jest/recommended", "plugin:jest/style"],
  "plugins": ["jest"],
  "rules": {
    "no-console": ["warn"],
    "no-empty": [
      "error",
      {
        "allowEmptyCatch": true
      }
    ]
  }
}
